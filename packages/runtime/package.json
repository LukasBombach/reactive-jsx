{
  "name": "@reactive-jsx/runtime",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "nodemon -w src -e ts -x \"yarn dev:bundle && yarn build:as-string\"",
    "dev:bundle": "microbundle -i src/index.ts -o dist/bundle.js --no-pkg-main --sourcemap false -f modern --compress false",
    "build": "yarn build:bundle && yarn build:as-string",
    "build:bundle": "microbundle -i src/index.ts -o dist/bundle.js --no-pkg-main -f modern",
    "build:as-string": "node scripts/exportAsString"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@reactive-jsx/config": "*",
    "@reactive-jsx/tsconfig": "*",
    "@types/jest": "^27.5.0",
    "@types/react": "17.0.39",
    "babel-jest": "^28.1.0",
    "jest": "^28.1.0",
    "microbundle": "0.14.2",
    "nodemon": "2.0.15",
    "typescript": "4.5.3"
  }
}
