{
  "name": "@reactive-jsx/runtime",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "nodemon --watch ./src --exec \"yarn dev:bundle && yarn build:as-string\"",
    "dev:bundle": "microbundle -i src/index.ts -o dist/bundle.js --no-pkg-main --compress false --sourcemap false --generateTypes false -f modern",
    "build": "yarn build:bundle && yarn build:as-string",
    "build:bundle": "microbundle -i src/index.ts -o dist/bundle.js --no-pkg-main -f modern",
    "build:as-string": "node scripts/exportAsString"
  },
  "devDependencies": {
    "@reactive-jsx/config": "*",
    "@reactive-jsx/tsconfig": "*",
    "@types/react": "17.0.39",
    "microbundle": "0.14.2",
    "nodemon": "2.0.15",
    "typescript": "4.5.3"
  }
}
